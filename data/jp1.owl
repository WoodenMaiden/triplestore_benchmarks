  <skos:prefLabel xml:lang="en">main supply route</skos:prefLabel>
  <foaf:name>main supply route</foaf:name>
  <rdfs:label xml:lang="en">main supply route (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The route or routes designated within an operational area upon which the bulk of traffic flows in support of military operations.</skos:definition>

  <skos:prefLabel xml:lang="en">target component</skos:prefLabel>
  <foaf:name>target component</foaf:name>
  <rdfs:label xml:lang="en">target component (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A set of targets within a target system performing a similar function.</skos:definition>
  <skos:related rdf:resource="#target"/>
  <skos:related rdf:resource="#target_critical_damage_point"/>
  <skos:inScheme rdf:resource="#JP_3-60Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="target_concentration">
  <dc:source rdf:resource="#JP_1-02DocPage311"/>
  <skos:prefLabel xml:lang="en">target concentration</skos:prefLabel>
  <foaf:name>target concentration</foaf:name>
  <rdfs:label xml:lang="en">target concentration (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A grouping of geographically proximate targets.</skos:definition>
  <skos:related rdf:resource="#target"/>
  <skos:related rdf:resource="#target_complex"/>
  <skos:inScheme rdf:resource="#JP_3-60Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="target_critical_damage_point">
  <dc:source rdf:resource="#JP_1-02DocPage311"/>
  <skos:prefLabel xml:lang="en">target critical damage point</skos:prefLabel>
  <foaf:name>target critical damage point</foaf:name>
  <rdfs:label xml:lang="en">target critical damage point (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The part of a target component that is most vital.</skos:definition>
  <skos:altLabel>critical node</skos:altLabel>
  <skos:related rdf:resource="#target"/>
  <skos:related rdf:resource="#target_component"/>
  <skos:inScheme rdf:resource="#JP_3-05.1Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="target_development">
  <dc:source rdf:resource="#JP_1-02DocPage311"/>
  <skos:prefLabel xml:lang="en">target development</skos:prefLabel>
  <foaf:name>target development</foaf:name>
  <rdfs:label xml:lang="en">target development (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The systematic examination of potential target systems - and their components, individual targets, and even elements of targets - to determine the necessary type and duration of the action that must be exerted on each target to create an effect that is consistent with the commander’s specific objectives.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-60Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="target_folder">
  <dc:source rdf:resource="#JP_1-02DocPage311"/>
  <skos:prefLabel xml:lang="en">target folder</skos:prefLabel>
  <foaf:name>target folder</foaf:name>
  <rdfs:label xml:lang="en">target folder (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A folder, hardcopy or electronic, containing target intelligence and related materials prepared for planning and executing action against a specific target.</skos:definition>
  <skos:related rdf:resource="#target"/>
  <skos:inScheme rdf:resource="#JP_3-60Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="ballistic_missile_early_warning_system">
  <dc:source rdf:resource="#JP_1-02DocPage29"/>
  <skos:prefLabel xml:lang="en">ballistic missile early warning system</skos:prefLabel>
  <foaf:name>ballistic missile early warning system</foaf:name>
  <rdfs:label xml:lang="en">ballistic missile early warning system (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">An electronic system for providing detection and early warning of attack by enemy intercontinental ballistic missiles.</skos:definition>
  <skos:altLabel>BMEWS</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_3-14Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="target_information_center">
  <dc:source rdf:resource="#JP_1-02DocPage311"/>
  <skos:prefLabel xml:lang="en">target information center</skos:prefLabel>
  <foaf:name>target information center</foaf:name>
  <rdfs:label xml:lang="en">target information center (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The agency or activity responsible for collecting, displaying, evaluating, and disseminating information pertaining to potential targets.</skos:definition>
  <skos:altLabel>TIC</skos:altLabel>
  <skos:related rdf:resource="#target"/>
  <skos:inScheme rdf:resource="#JP_3-02Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="targeting">
  <dc:source rdf:resource="#JP_1-02DocPage311"/>
  <skos:prefLabel xml:lang="en">targeting</skos:prefLabel>
  <foaf:name>targeting</foaf:name>
  <rdfs:label xml:lang="en">targeting (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The process of selecting and prioritizing targets and matching the appropriate response to them, considering operational requirements and capabilities.</skos:definition>
  <skos:related rdf:resource="#joint_targeting_coordination_board"/>
  <skos:related rdf:resource="#target"/>
  <skos:inScheme rdf:resource="#JP_3-0Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="target_intelligence">
  <dc:source rdf:resource="#JP_1-02DocPage311"/>
  <skos:prefLabel xml:lang="en">target intelligence</skos:prefLabel>
  <foaf:name>target intelligence</foaf:name>
  <rdfs:label xml:lang="en">target intelligence (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Intelligence that portrays and locates the components of a target or target complex and indicates its vulnerability and relative importance.</skos:definition>
  <skos:related rdf:resource="#target"/>
  <skos:related rdf:resource="#target_complex"/>
  <skos:inScheme rdf:resource="#JP_3-60Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="target_location_error">
  <dc:source rdf:resource="#JP_1-02DocPage312"/>
  <skos:prefLabel xml:lang="en">target location error</skos:prefLabel>
  <foaf:name>target location error</foaf:name>
  <rdfs:label xml:lang="en">target location error (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The difference between the coordinates generated for a target and the actual location of the target. Target location error is expressed primarily in terms of circular and vertical errors or infrequently, as spherical error.</skos:definition>
  <skos:altLabel>TLE</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_3-09.3Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="target_materials">
  <dc:source rdf:resource="#JP_1-02DocPage312"/>
  <skos:prefLabel xml:lang="en">target materials</skos:prefLabel>
  <foaf:name>target materials</foaf:name>
  <rdfs:label xml:lang="en">target materials (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Graphic, textual, tabular, digital, video, or other presentations of target intelligence, primarily designed to support operations against designated targets by one or more weapon(s) systems. Target materials are suitable for training, planning, executing, and evaluating military operations.</skos:definition>
  <skos:related rdf:resource="#air_target_materials_program"/>
  <skos:inScheme rdf:resource="#JP_2-0Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="target_nomination_list">
  <dc:source rdf:resource="#JP_1-02DocPage312"/>
  <skos:prefLabel xml:lang="en">target nomination list</skos:prefLabel>
  <foaf:name>target nomination list</foaf:name>
  <rdfs:label xml:lang="en">target nomination list (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A target-consolidated list of targets made up of the multiple candidate target lists. A prioritized list of targets drawn from the joint target list and nominated by component commanders, appropriate agencies, or the joint force commander’s staff for inclusion on the joint integrated prioritized target list.</skos:definition>
  <skos:altLabel>TNL</skos:altLabel>
  <skos:related rdf:resource="#candidate_target_list"/>
  <skos:related rdf:resource="#joint_integrated_prioritized_target_list"/>
  <skos:related rdf:resource="#target"/>
  <skos:inScheme rdf:resource="#JP_3-60Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="target_of_opportunity">
  <dc:source rdf:resource="#JP_1-02DocPage312"/>
  <skos:prefLabel xml:lang="en">target of opportunity</skos:prefLabel>
  <foaf:name>target of opportunity</foaf:name>

<skos:Concept rdf:ID="evaluation_agent">
  <dc:source rdf:resource="#JP_1-02DocPage111"/>
  <skos:prefLabel xml:lang="en">evaluation agent</skos:prefLabel>
  <foaf:name>evaluation agent</foaf:name>
  <rdfs:label xml:lang="en">evaluation agent (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The command or agency designated in the evaluation directive to be responsible for the planning, coordination, and conduct of the required evaluation of a joint test publication. (CJCSM 5120.01)</skos:definition>

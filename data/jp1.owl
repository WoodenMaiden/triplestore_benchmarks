<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="building_systems">
  <dc:source rdf:resource="#JP_1-02DocPage37"/>
  <skos:prefLabel xml:lang="en">building systems</skos:prefLabel>
  <foaf:name>building systems</foaf:name>
  <rdfs:label xml:lang="en">building systems (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Structures assembled from manufactured components designed to provide specific building configurations.</skos:definition>

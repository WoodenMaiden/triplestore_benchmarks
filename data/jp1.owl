<skos:Concept rdf:ID="rationalization">
  <dc:source rdf:resource="#JP_1-02DocPage262"/>
  <skos:prefLabel xml:lang="en">rationalization</skos:prefLabel>
  <foaf:name>rationalization</foaf:name>
  <rdfs:label xml:lang="en">rationalization (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Any action that increases the effectiveness of allied forces through more efficient or effective use of defense resources committed to the alliance. Rationalization includes consolidation, reassignment of national priorities to higher alliance needs, standardization, specialization, mutual support or improved interoperability, and greater cooperation. Rationalization applies to both weapons and/or materiel resources and non-weapons military matters.</skos:definition>
  <skos:inScheme rdf:resource="#JP_4-08Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="assault_follow-on_echelon">

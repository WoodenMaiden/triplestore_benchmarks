  <dc:source rdf:resource="#JP_1-02DocPage30"/>
  <skos:prefLabel xml:lang="en">base</skos:prefLabel>
  <foaf:name>base</foaf:name>
  <rdfs:label xml:lang="en">base (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:related rdf:resource="#facility"/>
  <skos:inScheme rdf:resource="#JP_4-0Scheme"/>

</skos:Concept>

<skos:Concept rdf:ID="transient_forces">
  <dc:source rdf:resource="#JP_1-02DocPage324"/>
  <skos:prefLabel xml:lang="en">transient forces</skos:prefLabel>
  <foaf:name>transient forces</foaf:name>

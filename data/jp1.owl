  <rdfs:label xml:lang="en">assault shipping (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Shipping assigned to the amphibious task force and utilized for transporting assault troops, vehicles, equipment, and supplies to the objective area.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-02Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>


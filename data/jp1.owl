  <foaf:name>demobilization</foaf:name>
  <rdfs:label xml:lang="en">demobilization (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The process of transitioning a conflict or wartime military establishment and defense-based civilian economy to a peacetime configuration while maintaining national security and economic vitality.</skos:definition>
  <skos:related rdf:resource="#mobilization"/>
  <skos:inScheme rdf:resource="#JP_4-05Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>

    <owl:differentFrom rdf:resource="#identification_2"/>
    <skos:broader rdf:resource="#identification"/>
    <skos:definition xml:lang="en">In ground combat operations, discrimination between recognizable objects as being friendly or enemy, or the name that belongs to the object as a member of a class.</skos:definition>
    <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
    <skos:inScheme rdf:resource="#JP_3-01Scheme"/>

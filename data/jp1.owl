<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="bare_base">
  <dc:source rdf:resource="#JP_1-02DocPage29"/>
  <skos:prefLabel xml:lang="en">bare base</skos:prefLabel>
  <foaf:name>bare base</foaf:name>
  <rdfs:label xml:lang="en">bare base (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A base having minimum essential facilities to house, sustain, and support operations to include, if required, a stabilized runway, taxiways, and aircraft parking areas. A bare base must have a source of water that can be made potable. Other requirements to operate under bare base conditions form a necessary part of the force package deployed to the bare base.</skos:definition>
  <skos:related rdf:resource="#base"/>
  <skos:inScheme rdf:resource="#JP_3-05.1Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>


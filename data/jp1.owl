<skos:Concept rdf:ID="demilitarized_zone">
  <dc:source rdf:resource="#JP_1-02DocPage88"/>
  <skos:prefLabel xml:lang="en">demilitarized zone</skos:prefLabel>
  <foaf:name>demilitarized zone</foaf:name>
  <rdfs:label xml:lang="en">demilitarized zone (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A defined area in which the stationing or concentrating of military forces, or the retention or establishment of military installations of any description, is prohibited.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-07.3Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="demobilization">

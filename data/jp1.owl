<skos:Concept rdf:ID="coup_de_main">
  <dc:source rdf:resource="#JP_1-02DocPage75"/>
  <skos:prefLabel xml:lang="en">coup de main</skos:prefLabel>
  <foaf:name>coup de main</foaf:name>
  <rdfs:label xml:lang="en">coup de main (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">An offensive operation that capitalizes on surprise and simultaneous execution of supporting operations to achieve success in one swift stroke.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-0Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="courier">
  <dc:source rdf:resource="#JP_1-02DocPage75"/>
  <skos:prefLabel xml:lang="en">courier</skos:prefLabel>
  <foaf:name>courier</foaf:name>
  <rdfs:label xml:lang="en">courier (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A messenger (usually a commissioned or warrant officer) responsible for the secure physical transmission and delivery of documents and material.</skos:definition>
  <skos:inScheme rdf:resource="#JP_2-01Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>


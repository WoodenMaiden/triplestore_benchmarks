  <foaf:name>desired mean point of impact</foaf:name>
  <rdfs:label xml:lang="en">desired mean point of impact (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A precise point, associated with a target, and assigned as the center for impact of multiple weapons or area munitions to create a desired effect. May be defined descriptively, by grid reference, or by geolocation.</skos:definition>
  <skos:altLabel>DMPI</skos:altLabel>
  <skos:related rdf:resource="#aimpoint"/>
  <skos:related rdf:resource="#desired_point_of_impact"/>
  <skos:inScheme rdf:resource="#JP_3-60Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>


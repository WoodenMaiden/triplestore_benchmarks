  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The basic reference document for personnel recovery-specific information on a particular country or region of interest.</skos:definition>
  <skos:altLabel>JPRSP</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_3-50Scheme"/>

  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">An area in which specific restrictions are imposed and into which fires that exceed those restrictions will not be delivered without coordination with the establishing headquarters.</skos:definition>

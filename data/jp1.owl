<skos:Concept rdf:ID="map_chart">
  <dc:source rdf:resource="#JP_1-02DocPage194"/>
  <skos:prefLabel xml:lang="en">map chart</skos:prefLabel>
  <foaf:name>map chart</foaf:name>
  <rdfs:label xml:lang="en">map chart (JP 1-02)</rdfs:label>

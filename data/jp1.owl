<skos:Concept rdf:ID="radio_frequency_countermeasures">
  <dc:source rdf:resource="#JP_1-02DocPage261"/>
  <skos:prefLabel xml:lang="en">radio frequency countermeasures</skos:prefLabel>
  <foaf:name>radio frequency countermeasures</foaf:name>
  <rdfs:label xml:lang="en">radio frequency countermeasures (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Any device or technique employing radio frequency materials or technology that is intended to impair the effectiveness of enemy activity, particularly with respect to precision guided weapons and sensor systems.</skos:definition>

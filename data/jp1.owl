  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A series of related major operations aimed at achieving strategic and operational objectives within a given time and space.</skos:definition>
  <skos:related rdf:resource="#campaign_plan"/>
  <skos:inScheme rdf:resource="#JP_5-0Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="campaign_plan">

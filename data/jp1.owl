<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="measure_of_performance">
  <dc:source rdf:resource="#JP_1-02DocPage199"/>
  <skos:prefLabel xml:lang="en">measure of performance</skos:prefLabel>
  <foaf:name>measure of performance</foaf:name>
  <rdfs:label xml:lang="en">measure of performance (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A criterion used to assess friendly actions that is tied to measuring task accomplishment.</skos:definition>
  <skos:altLabel>MOP</skos:altLabel>

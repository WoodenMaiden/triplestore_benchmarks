  <skos:prefLabel xml:lang="en">strafing</skos:prefLabel>
  <foaf:name>strafing</foaf:name>
  <rdfs:label xml:lang="en">strafing (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The delivery of automatic weapons fire by aircraft on ground targets.</skos:definition>

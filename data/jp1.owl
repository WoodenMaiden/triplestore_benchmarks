<skos:Concept rdf:ID="clearance_capacity">
  <dc:source rdf:resource="#JP_1-02DocPage49"/>
  <skos:prefLabel xml:lang="en">clearance capacity</skos:prefLabel>
  <foaf:name>clearance capacity</foaf:name>
  <rdfs:label xml:lang="en">clearance capacity (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">An estimate expressed in agreed upon units of cargo measurement per day of the cargo or people that may be transported inland from a beach or port over the available means of inland communication, including roads, railroads, airlift, and inland waterways.</skos:definition>
  <skos:related rdf:resource="#throughput_capacity"/>
  <skos:inScheme rdf:resource="#JP_4-01.5Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="clearance_decontamination">
  <dc:source rdf:resource="#JP_1-02DocPage49"/>
  <skos:prefLabel xml:lang="en">clearance decontamination</skos:prefLabel>
  <foaf:name>clearance decontamination</foaf:name>
  <rdfs:label xml:lang="en">clearance decontamination (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The final level of decontamination that provides the decontamination of equipment and personnel to a level that allows unrestricted transportation, maintenance, employment, and disposal.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-11Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="afloat_pre-positioning_force">

  <skos:inScheme rdf:resource="#JP_3-61Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="media_pool">
  <dc:source rdf:resource="#JP_1-02DocPage200"/>
  <skos:prefLabel xml:lang="en">media pool</skos:prefLabel>
  <foaf:name>media pool</foaf:name>
  <rdfs:label xml:lang="en">media pool (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A limited number of news media who represent a larger number of news media organizations for purposes of news gathering and sharing of material during a specified activity. Pooling is typically used when news media support resources cannot accommodate a large number of journalists.</skos:definition>

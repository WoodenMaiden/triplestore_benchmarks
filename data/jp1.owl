  <skos:inScheme rdf:resource="#JP_3-17Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="active_defense">
  <dc:source rdf:resource="#JP_1-02DocPage2"/>
  <skos:prefLabel xml:lang="en">active defense</skos:prefLabel>
  <foaf:name>active defense</foaf:name>
  <rdfs:label xml:lang="en">active defense (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The employment of limited offensive action and counterattacks to deny a contested area or position to the enemy.</skos:definition>
  <skos:related rdf:resource="#passive_defense"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="airspace_management">
  <dc:source rdf:resource="#JP_1-02DocPage14"/>
  <skos:prefLabel xml:lang="en">airspace management</skos:prefLabel>
  <foaf:name>airspace management</foaf:name>
  <rdfs:label xml:lang="en">airspace management (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The coordination, integration, and regulation of the use of airspace of defined dimensions.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-52Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>

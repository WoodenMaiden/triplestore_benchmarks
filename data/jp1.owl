  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A special operations liaison team provided by the joint force special operations component commander to the joint force air component commander (if designated), or appropriate Service component air command and control organization, to coordinate, deconflict, and integrate special operations air, surface, and subsurface operations with conventional air operations.</skos:definition>
  <skos:altLabel>SOLE</skos:altLabel>
  <skos:related rdf:resource="#joint_force_air_component_commander"/>
  <skos:related rdf:resource="#joint_force_special_operations_component_commander"/>
  <skos:related rdf:resource="#special_operations"/>
  <skos:inScheme rdf:resource="#JP_3-05Scheme"/>

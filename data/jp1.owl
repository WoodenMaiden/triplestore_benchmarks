  <rdfs:label xml:lang="en">amphibious assault ship (general purpose) (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A naval ship designed to embark, deploy, and land elements of a landing force in an assault by helicopters, landing craft, amphibious vehicles, and by combinations of these methods.</skos:definition>
  <skos:altLabel>LHA</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_3-04Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="joint_operations_center">

  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A planning construct intended to facilitate early decision making by developing a wide range of interrelated responses that begin with deterrentoriented actions carefully tailored to produce a desired effect.</skos:definition>
  <skos:altLabel>FDO</skos:altLabel>
  <skos:related rdf:resource="#deterrent_options"/>
  <skos:inScheme rdf:resource="#JP_5-0Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>


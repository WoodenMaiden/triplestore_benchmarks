  <foaf:name>joint personnel recovery center</foaf:name>
  <rdfs:label xml:lang="en">joint personnel recovery center (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The primary joint force organization responsible for planning and coordinating personnel recovery for military operations within the assigned operational area.</skos:definition>
  <skos:altLabel>JPRC</skos:altLabel>
  <skos:related rdf:resource="#combat_search_and_rescue"/>

  <rdfs:label xml:lang="en">measure of effectiveness (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A criterion used to assess changes in system behavior, capability, or operational environment that is tied to measuring the attainment of an end state, achievement of an objective, or creation of an effect.</skos:definition>
  <skos:altLabel>MOE</skos:altLabel>
  <skos:related rdf:resource="#combat_assessment"/>

  <rdfs:label xml:lang="en">precedence (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
<owl:oneOf>
   <rdf:List rdf:ID="precedence_list">
    <rdf:first rdf:resource="#precedence_1"/>
    <rdf:rest>
     <rdf:List rdf:ID="precedence_list_1">  
      <rdf:first rdf:resource="#precedence_2"/>  
      <rdf:rest rdf:resource="#precedence_3"/>
     </rdf:List>
    </rdf:rest>
   </rdf:List>
</owl:oneOf>
<skos:narrower rdf:resource="#precedence_1"/>
<skos:narrower rdf:resource="#precedence_2"/>
<skos:narrower rdf:resource="#precedence_3"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>

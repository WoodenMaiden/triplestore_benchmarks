  <skos:prefLabel xml:lang="en">cyberspace</skos:prefLabel>
  <foaf:name>cyberspace</foaf:name>
  <rdfs:label xml:lang="en">cyberspace (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A global domain within the information environment consisting of the interdependent network of information technology infrastructures, including the 79 Internet, telecommunications networks, computer systems, and embedded processors and controllers. (CJCS CM-0363-08)</skos:definition>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="cyberspace_operations">

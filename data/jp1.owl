<skos:Concept rdf:ID="directive_authority_for_logistics">
  <dc:source rdf:resource="#JP_1-02DocPage94"/>
  <skos:prefLabel xml:lang="en">directive authority for logistics</skos:prefLabel>
  <foaf:name>directive authority for logistics</foaf:name>
  <rdfs:label xml:lang="en">directive authority for logistics (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Combatant commander authority to issue directives to subordinate commanders, including peacetime measures, necessary to ensure the effective execution of approved operation plans. Essential measures include the optimized use or reallocation of available resources and prevention or elimination of redundant facilities and/or overlapping functions among the Service component commands.</skos:definition>
  <skos:altLabel>DAFL</skos:altLabel>
  <skos:related rdf:resource="#combatant_command_command_authority"/>
  <skos:related rdf:resource="#logistics"/>
  <skos:inScheme rdf:resource="#JP_1Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="direct_laying">
  <dc:source rdf:resource="#JP_1-02DocPage94"/>

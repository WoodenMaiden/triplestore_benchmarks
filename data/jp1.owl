  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">1. Designated area(s) to which noncombatants of the United States Government’s responsibility and commercial vehicles and materiel may be evacuated during a domestic or other valid emergency.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-68Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>

       </rdf:List>
      </rdf:rest>
     </rdf:List>
    </rdf:rest>
   </rdf:List>
  </owl:oneOf>
<skos:narrower rdf:resource="#section_1"/>
<skos:narrower rdf:resource="#section_2"/>

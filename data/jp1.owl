<skos:Concept rdf:ID="tempest">
  <dc:source rdf:resource="#JP_1-02DocPage315"/>
  <skos:prefLabel xml:lang="en">tempest</skos:prefLabel>
  <foaf:name>tempest</foaf:name>
  <rdfs:label xml:lang="en">tempest (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">An unclassified term referring to technical investigations for compromising emanations from electrically operated information processing equipment; these investigations are conducted in support of emanations and emissions security.</skos:definition>
  <skos:related rdf:resource="#counterintelligence"/>
  <skos:inScheme rdf:resource="#JP_2-01.2Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="temporary_interment">
  <dc:source rdf:resource="#JP_1-02DocPage316"/>
  <skos:prefLabel xml:lang="en">temporary interment</skos:prefLabel>
  <foaf:name>temporary interment</foaf:name>
  <rdfs:label xml:lang="en">temporary interment (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A site for the purpose of: a. the interment of the remains if the circumstances permit; or b. the reburial of remains exhumed from an emergency interment.</skos:definition>
  <skos:related rdf:resource="#mortuary_affairs"/>
  <skos:inScheme rdf:resource="#JP_4-06Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="terminal">
  <dc:source rdf:resource="#JP_1-02DocPage316"/>
  <skos:prefLabel xml:lang="en">terminal</skos:prefLabel>
  <foaf:name>terminal</foaf:name>
  <rdfs:label xml:lang="en">terminal (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A facility designed to transfer cargo from one means of conveyance to another. (Conveyance is the piece of equipment used to transport cargo; i.e., railcar to truck or truck to truck. This is as opposed to mode, which is the type of equipment; i.e., ship to rail, rail to truck.)</skos:definition>
  <skos:related rdf:resource="#facility"/>
  <skos:inScheme rdf:resource="#JP_4-01.6Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="terminal_attack_control">
  <dc:source rdf:resource="#JP_1-02DocPage316"/>
  <skos:prefLabel xml:lang="en">terminal attack control</skos:prefLabel>
  <foaf:name>terminal attack control</foaf:name>
  <rdfs:label xml:lang="en">terminal attack control (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The authority to control the maneuver of and grant weapons release clearance to attacking aircraft.</skos:definition>
  <skos:related rdf:resource="#joint_terminal_attack_controller"/>
  <skos:inScheme rdf:resource="#JP_3-09.3Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>


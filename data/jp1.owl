<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="high-altitude_low-opening_parachute_technique">
  <dc:source rdf:resource="#JP_1-02DocPage140"/>
  <skos:prefLabel xml:lang="en">high-altitude low-opening parachute technique</skos:prefLabel>
  <foaf:name>high-altitude low-opening parachute technique</foaf:name>
  <rdfs:label xml:lang="en">high-altitude low-opening parachute technique (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A method of delivering personnel, equipment, or supplies from airlift aircraft that must fly at altitudes above the threat umbrella.</skos:definition>

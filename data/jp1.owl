      <rdf:rest rdf:resource="#base_3"/>
     </rdf:List>
    </rdf:rest>
   </rdf:List>
  </owl:oneOf>
<skos:narrower rdf:resource="#base_1"/>
<skos:narrower rdf:resource="#base_2"/>

  <skos:prefLabel xml:lang="en">joint interrogation operations</skos:prefLabel>
  <foaf:name>joint interrogation operations</foaf:name>
  <rdfs:label xml:lang="en">joint interrogation operations (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:altLabel>JIO</skos:altLabel>
  <skos:related rdf:resource="#enemy_combatant"/>

      <rdf:first rdf:resource="#neutralize_2"/>
      <rdf:rest>
       <rdf:List rdf:ID="neutralize_list_3">
        <rdf:first rdf:resource="#neutralize_3"/>
        <rdf:rest rdf:resource="#neutralize_4"/>
       </rdf:List>
      </rdf:rest>
     </rdf:List>
    </rdf:rest>
   </rdf:List>

  <skos:related rdf:resource="#integration"/>
  <skos:related rdf:resource="#joint_force"/>
  <skos:related rdf:resource="#reception"/>
  <skos:related rdf:resource="#staging"/>
  <skos:inScheme rdf:resource="#JP_3-35Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="joint_restricted_frequency_list">

  <skos:prefLabel xml:lang="en">subversion</skos:prefLabel>
  <foaf:name>subversion</foaf:name>
  <rdfs:label xml:lang="en">subversion (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Actions designed to undermine the military, economic, psychological, or political strength or morale of a governing authority.</skos:definition>

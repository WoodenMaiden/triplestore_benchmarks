<skos:Concept rdf:ID="airspace_coordination_area">
  <dc:source rdf:resource="#JP_1-02DocPage13"/>
  <skos:prefLabel xml:lang="en">airspace coordination area</skos:prefLabel>
  <foaf:name>airspace coordination area</foaf:name>
  <rdfs:label xml:lang="en">airspace coordination area (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A three-dimensional block of airspace in a target area, established by the appropriate ground commander, in which friendly aircraft are reasonably safe from friendly surface fires. The airspace coordination area may be formal or informal.</skos:definition>
  <skos:altLabel>ACA</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_3-09.3Scheme"/>

<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

  <skos:Concept rdf:ID="pressure_mine_2">
    <dc:source rdf:resource="#JP_1-02DocPage250"/>
    <rdfs:label xml:lang="en">pressure mine (Naval)</rdfs:label>
    <owl:differentFrom rdf:resource="#pressure_mine_1"/>
    <skos:broader rdf:resource="#pressure_mine"/>
    <skos:definition xml:lang="en">In naval mine warfare, a mine whose circuit responds to the hydrodynamic pressure field of a target.</skos:definition>
    <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
    <skos:inScheme rdf:resource="#JP_3-15Scheme"/>
    <skos:prefLabel xml:lang="en">pressure mine</skos:prefLabel>

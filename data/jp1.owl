  <foaf:name>short-range ballistic missile</foaf:name>
  <rdfs:label xml:lang="en">short-range ballistic missile (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A land-based ballistic missile with a range capability up to about 600 nautical miles.</skos:definition>

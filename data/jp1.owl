  <skos:prefLabel xml:lang="en">integrated staff</skos:prefLabel>
  <foaf:name>integrated staff</foaf:name>
  <rdfs:label xml:lang="en">integrated staff (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">(*) A staff in which one officer only is appointed to each post on the establishment of the headquarters, irrespective of nationality and Service.</skos:definition>

<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="denied_area">
  <dc:source rdf:resource="#JP_1-02DocPage88"/>
  <skos:prefLabel xml:lang="en">denied area</skos:prefLabel>
  <foaf:name>denied area</foaf:name>
  <rdfs:label xml:lang="en">denied area (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">An area under enemy or unfriendly control in which friendly forces cannot expect to operate successfully within existing operational constraints and force capabilities.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-05Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="department_of_defense_civilian">
  <dc:source rdf:resource="#JP_1-02DocPage88"/>
  <skos:prefLabel xml:lang="en">Department of Defense civilian</skos:prefLabel>
  <foaf:name>Department of Defense civilian</foaf:name>
  <rdfs:label xml:lang="en">Department of Defense civilian (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A Federal civilian employee of the Department of Defense directly hired and paid from appropriated or nonappropriated funds, under permanent or temporary appointment.</skos:definition>
  <skos:inScheme rdf:resource="#JP_1-0Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="department_of_defense_components">
  <dc:source rdf:resource="#JP_1-02DocPage88"/>

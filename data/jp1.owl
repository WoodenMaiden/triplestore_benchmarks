<skos:Concept rdf:ID="naval_special_warfare_unit">
  <dc:source rdf:resource="#JP_1-02DocPage221"/>
  <skos:prefLabel xml:lang="en">naval special warfare unit</skos:prefLabel>
  <foaf:name>naval special warfare unit</foaf:name>
  <rdfs:label xml:lang="en">naval special warfare unit (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A permanent Navy organization forward based to control and support attached naval special warfare forces.</skos:definition>
  <skos:altLabel>NSWU</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_3-05.1Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="naval_surface_fire_support">
  <dc:source rdf:resource="#JP_1-02DocPage221"/>
  <skos:prefLabel xml:lang="en">naval surface fire support</skos:prefLabel>
  <foaf:name>naval surface fire support</foaf:name>
  <rdfs:label xml:lang="en">naval surface fire support (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Fire provided by Navy surface gun and missile systems in support of a unit or units.</skos:definition>
  <skos:altLabel>NSFS</skos:altLabel>

  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The process whereby isolated personnel avoid capture with the goal of successfully returning to areas under friendly control.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-50Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="evasion_aid">
  <dc:source rdf:resource="#JP_1-02DocPage112"/>
  <skos:prefLabel xml:lang="en">evasion aid</skos:prefLabel>

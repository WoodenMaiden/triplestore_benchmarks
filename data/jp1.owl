         </rdf:List>
        </rdf:rest>
       </rdf:List>
      </rdf:rest>
     </rdf:List>
    </owl:oneOf>
   <skos:narrower rdf:resource="#tactical_warning_2a"/>
   <skos:narrower rdf:resource="#tactical_warning_2b"/>

<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="returnee">
  <dc:source rdf:resource="#JP_1-02DocPage273"/>
  <skos:prefLabel xml:lang="en">returnee</skos:prefLabel>
  <foaf:name>returnee</foaf:name>

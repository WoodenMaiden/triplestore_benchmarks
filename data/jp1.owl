    <rdf:rest rdf:resource="#warning_2"/>
   </rdf:List>
</owl:oneOf>
<skos:narrower rdf:resource="#warning_1"/>
<skos:narrower rdf:resource="#warning_2"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="warning_order">
  <dc:source rdf:resource="#JP_1-02DocPage337"/>
  <skos:prefLabel xml:lang="en">warning order</skos:prefLabel>
  <foaf:name>warning order</foaf:name>
  <rdfs:label xml:lang="en">warning order (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:altLabel>WARNORD</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_5-0Scheme"/>
  <owl:oneOf>
   <rdf:List rdf:ID="warning_order_list">

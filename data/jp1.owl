  <skos:related rdf:resource="#amphibious_operation"/>
  <skos:related rdf:resource="#amphibious_task_force"/>
  <skos:related rdf:resource="#task_organization"/>
  <skos:inScheme rdf:resource="#JP_3-02Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="landing_force_operational_reserve_material">
  <dc:source rdf:resource="#JP_1-02DocPage184"/>
  <skos:prefLabel xml:lang="en">landing force operational reserve material</skos:prefLabel>

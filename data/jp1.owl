  <foaf:name>fleet</foaf:name>
  <rdfs:label xml:lang="en">fleet (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">An organization of ships, aircraft, Marine forces, and shore-based fleet activities all under the command of a commander who may exercise operational as well as administrative control.</skos:definition>

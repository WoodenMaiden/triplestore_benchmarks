<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="joint_personnel_reception_center">
  <dc:source rdf:resource="#JP_1-02DocPage175"/>
  <skos:prefLabel xml:lang="en">joint personnel reception center</skos:prefLabel>
  <foaf:name>joint personnel reception center</foaf:name>
  <rdfs:label xml:lang="en">joint personnel reception center (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A center established in an operational area by the appropriate joint force commander with the responsibility for the in-processing and outprocessing of personnel upon their arrival in and departure from the theater.</skos:definition>
  <skos:altLabel>JPRC</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_1-0Scheme"/>

<skos:Concept rdf:ID="nap-of-the-earth_flight">
  <dc:source rdf:resource="#JP_1-02DocPage215"/>
  <skos:prefLabel xml:lang="en">nap-of-the-earth flight</skos:prefLabel>
  <foaf:name>nap-of-the-earth flight</foaf:name>
  <rdfs:label xml:lang="en">nap-of-the-earth flight (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">See terrain flight.</skos:definition>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>

  <foaf:name>closure</foaf:name>
  <rdfs:label xml:lang="en">closure (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">In transportation, the process of a unit’s arriving at a specified location.</skos:definition>
  <skos:inScheme rdf:resource="#JP_401.5Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="coalition">
  <dc:source rdf:resource="#JP_1-02DocPage49"/>
  <skos:prefLabel xml:lang="en">coalition</skos:prefLabel>
  <foaf:name>coalition</foaf:name>
  <rdfs:label xml:lang="en">coalition (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">An arrangement between two or more nations for common action.</skos:definition>
  <skos:related rdf:resource="#alliance"/>
  <skos:related rdf:resource="#multinational"/>
  <skos:inScheme rdf:resource="#JP_5-0Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="coastal_sea_control">
  <dc:source rdf:resource="#JP_1-02DocPage49"/>
  <skos:prefLabel xml:lang="en">coastal sea control</skos:prefLabel>
  <foaf:name>coastal sea control</foaf:name>
  <rdfs:label xml:lang="en">coastal sea control (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The employment of forces to ensure the unimpeded use of an offshore coastal area by friendly forces and, as appropriate, to deny the use of the area to enemy forces.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-10Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>


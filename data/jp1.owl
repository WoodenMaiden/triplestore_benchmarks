  <skos:related rdf:resource="#combat_service_support"/>
  <skos:related rdf:resource="#combat_support"/>
  <skos:related rdf:resource="#landing_force"/>
  <skos:related rdf:resource="#ship-to-shore_movement"/>
  <skos:inScheme rdf:resource="#JP_3-02Scheme"/>

<skos:Concept rdf:ID="heavy-lift_ship">
  <dc:source rdf:resource="#JP_1-02DocPage139"/>
  <skos:prefLabel xml:lang="en">heavy-lift ship</skos:prefLabel>
  <foaf:name>heavy-lift ship</foaf:name>
  <rdfs:label xml:lang="en">heavy-lift ship (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A ship specially designed and capable of loading and unloading heavy and bulky items and has booms of sufficient capacity to accommodate a single lift of 100 tons.</skos:definition>
  <skos:inScheme rdf:resource="#JP_4-01.2Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="height_of_burst">
  <dc:source rdf:resource="#JP_1-02DocPage139"/>
  <skos:prefLabel xml:lang="en">height of burst</skos:prefLabel>
  <foaf:name>height of burst</foaf:name>
  <rdfs:label xml:lang="en">height of burst (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The vertical distance from the Earth’s surface or target to the point of burst.</skos:definition>
  <skos:altLabel>HOB</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_3-41Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="helicopter_coordination_section">
  <dc:source rdf:resource="#JP_1-02DocPage139"/>
  <skos:prefLabel xml:lang="en">helicopter coordination section</skos:prefLabel>
  <foaf:name>helicopter coordination section</foaf:name>
  <rdfs:label xml:lang="en">helicopter coordination section (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The section within the Navy tactical air control center that coordinates rotary-wing air operations with all helicopter direction centers and air traffic control center(s) in the amphibious force.</skos:definition>
  <skos:altLabel>HCS</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_3-02Scheme"/>

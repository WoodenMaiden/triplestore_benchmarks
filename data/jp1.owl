</skos:Concept>

<skos:Concept rdf:ID="economic_action">
  <dc:source rdf:resource="#JP_1-02DocPage101"/>
  <skos:prefLabel xml:lang="en">economic action</skos:prefLabel>
  <foaf:name>economic action</foaf:name>
  <rdfs:label xml:lang="en">economic action (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The planned use of economic measures designed to influence the policies or actions of another state, e.g., to impair the war-making potential of a hostile power or to generate economic stability within a friendly power.</skos:definition>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="economy_of_force">
  <dc:source rdf:resource="#JP_1-02DocPage101"/>
  <skos:prefLabel xml:lang="en">economy of force</skos:prefLabel>
  <foaf:name>economy of force</foaf:name>
  <rdfs:label xml:lang="en">economy of force (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The judicious employment and distribution of forces so as to expend the minimum essential combat power on secondary efforts in order to allocate the maximum possible combat power on primary efforts.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-0Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="e-day">
  <dc:source rdf:resource="#JP_1-02DocPage101"/>
  <skos:prefLabel xml:lang="en">E-day</skos:prefLabel>
  <foaf:name>E-day</foaf:name>
  <rdfs:label xml:lang="en">E-day (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">See times.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-02.1Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>


  <skos:altLabel>CMD</skos:altLabel>
  <skos:related rdf:resource="#area_command"/>
  <skos:related rdf:resource="#combatant_command"/>
  <skos:related rdf:resource="#combatant_command_command_authority"/>
  <skos:inScheme rdf:resource="#JP_1Scheme"/>
  <owl:oneOf>
   <rdf:List rdf:ID="command_list">
    <rdf:first rdf:resource="#command_1"/>
    <rdf:rest>
     <rdf:List rdf:ID="command_list_1">
      <rdf:first rdf:resource="#command_2"/>

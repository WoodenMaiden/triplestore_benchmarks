<skos:Concept rdf:ID="denial_measure">
  <dc:source rdf:resource="#JP_1-02DocPage88"/>
  <skos:prefLabel xml:lang="en">denial measure</skos:prefLabel>
  <foaf:name>denial measure</foaf:name>
  <rdfs:label xml:lang="en">denial measure (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">An action to hinder or deny the enemy the use of territory, personnel, or facilities to include destruction, removal, contamination, or erection of obstructions.</skos:definition>

<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="bulk_petroleum_product">
  <dc:source rdf:resource="#JP_1-02DocPage37"/>
  <skos:prefLabel xml:lang="en">bulk petroleum product</skos:prefLabel>
  <foaf:name>bulk petroleum product</foaf:name>
  <rdfs:label xml:lang="en">bulk petroleum product (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A liquid petroleum product transported by various means and stored in tanks or containers having an individual fill capacity greater than 250 liters.</skos:definition>
  <skos:inScheme rdf:resource="#JP_4-03Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="aegis">

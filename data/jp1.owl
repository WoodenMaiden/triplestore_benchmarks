<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="track_correlation">
  <dc:source rdf:resource="#JP_1-02DocPage323"/>
  <skos:prefLabel xml:lang="en">track correlation</skos:prefLabel>
  <foaf:name>track correlation</foaf:name>
  <rdfs:label xml:lang="en">track correlation (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Correlating track information for identification purposes using all available data.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-01Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>

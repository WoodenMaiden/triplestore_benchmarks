  <foaf:name>joint operations area forecast</foaf:name>
  <rdfs:label xml:lang="en">joint operations area forecast (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The official baseline meteorological and oceanographic forecast for operational planning and mission execution within the joint operations area.</skos:definition>
  <skos:altLabel>JOAF</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_3-59Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>

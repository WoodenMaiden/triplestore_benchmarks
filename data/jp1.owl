<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="special_operations_weather_team">
  <dc:source rdf:resource="#JP_1-02DocPage294"/>
  <skos:prefLabel xml:lang="en">special operations weather team</skos:prefLabel>
  <foaf:name>special operations weather team</foaf:name>
  <rdfs:label xml:lang="en">special operations weather team (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A task organized team of Air Force personnel organized, trained, and equipped to collect critical environmental information from data sparse areas.</skos:definition>

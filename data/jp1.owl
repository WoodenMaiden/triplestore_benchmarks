  <foaf:name>shore party</foaf:name>
  <rdfs:label xml:lang="en">shore party (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A task organization of the landing force, formed for the purpose of facilitating the landing and movement off the beaches of troops, equipment, and supplies; for the evacuation from the beaches of casualties and enemy prisoners of war; and for facilitating the beaching, retraction, and salvaging of landing ships and craft. It comprises elements of both the naval and landing forces.</skos:definition>
  <skos:altLabel>beach group</skos:altLabel>
  <skos:related rdf:resource="#beachmaster_unit"/>
  <skos:related rdf:resource="#beach_party"/>
  <skos:related rdf:resource="#naval_beach_group"/>
  <skos:inScheme rdf:resource="#JP_3-02Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>


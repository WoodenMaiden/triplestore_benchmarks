  <rdfs:label xml:lang="en">desired point of impact (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A precise point, associated with a target, and assigned as the impact point for a single unitary weapon to create a desired effect. May be defined descriptively, by grid preferences, or geolocation.</skos:definition>
  <skos:altLabel>DPI</skos:altLabel>
  <skos:related rdf:resource="#aimpoint"/>
  <skos:related rdf:resource="#desired_mean_point_of_impact"/>

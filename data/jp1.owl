  <foaf:name>joint planning and execution community</foaf:name>
  <rdfs:label xml:lang="en">joint planning and execution community (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Those headquarters, commands, and agencies involved in the training, preparation, mobilization, deployment, employment, support, sustainment, redeployment, and demobilization of military forces assigned or committed to a joint operation.</skos:definition>
  <skos:altLabel>JPEC</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_5-0Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>


<skos:Concept rdf:ID="grid_bearing">
  <dc:source rdf:resource="#JP_1-02DocPage134"/>
  <skos:prefLabel xml:lang="en">grid bearing</skos:prefLabel>
  <foaf:name>grid bearing</foaf:name>

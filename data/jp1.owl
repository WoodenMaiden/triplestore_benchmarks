  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A prioritized list of high-payoff targets by phase of the joint operation.</skos:definition>
  <skos:altLabel>HPTL</skos:altLabel>
  <skos:related rdf:resource="#high-payoff_target"/>
  <skos:related rdf:resource="#target"/>
  <skos:inScheme rdf:resource="#JP_3-60Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>

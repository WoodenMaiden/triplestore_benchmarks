  <skos:inScheme rdf:resource="#JP_3-0Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="operational_limitation">
  <dc:source rdf:resource="#JP_1-02DocPage234"/>
  <skos:prefLabel xml:lang="en">operational limitation</skos:prefLabel>
  <foaf:name>operational limitation</foaf:name>
  <rdfs:label xml:lang="en">operational limitation (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">An action required or prohibited by higher authority, such as a constraint or a restraint, and other restrictions that limit the commander’s freedom of action, such as diplomatic agreements, rules of engagement, political and economic conditions in affected countries, and host nation issues.</skos:definition>
  <skos:related rdf:resource="#constraint"/>
  <skos:related rdf:resource="#restraint"/>
  <skos:inScheme rdf:resource="#JP_5-0Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="operationally_ready">
  <dc:source rdf:resource="#JP_1-02DocPage234"/>
  <skos:prefLabel xml:lang="en">operationally ready</skos:prefLabel>
  <foaf:name>operationally ready</foaf:name>
  <rdfs:label xml:lang="en">operationally ready (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:inScheme rdf:resource="#JP_4-01.5Scheme"/>

  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Support provided by the Department of Defense to law enforcement agencies to detect, monitor, and counter the production, trafficking, and use of illegal drugs.</skos:definition>

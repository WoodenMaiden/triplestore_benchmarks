<skos:Concept rdf:ID="revolving_fund_account">
  <dc:source rdf:resource="#JP_1-02DocPage273"/>
  <skos:prefLabel xml:lang="en">revolving fund account</skos:prefLabel>
  <foaf:name>revolving fund account</foaf:name>
  <rdfs:label xml:lang="en">revolving fund account (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">An account authorized by specific provisions of law to finance a continuing cycle of business-type operations, and which are authorized to incur obligations and expenditures that generate receipts.</skos:definition>

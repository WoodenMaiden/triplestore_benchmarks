    <owl:differentFrom rdf:resource="#integration_1"/>
    <owl:differentFrom rdf:resource="#integration_2"/>
    <skos:broader rdf:resource="#integration"/>
    <skos:definition xml:lang="en">In photography, a process by which the average radar picture seen on several scans of the 155 time base may be obtained on a print, or the process by which several photographic images are combined into a single image.</skos:definition>
    <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
    <skos:inScheme rdf:resource="#JP_1Scheme"/>
    <skos:prefLabel xml:lang="en">integration</skos:prefLabel>
    <skos:related rdf:resource="#force_protection"/>
    <foaf:name>integration</foaf:name>
  <rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

  <skos:Concept rdf:ID="intelligence_requirement_1">

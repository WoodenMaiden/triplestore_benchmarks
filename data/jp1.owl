<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="inventory_managers">
  <dc:source rdf:resource="#JP_1-02DocPage163"/>
  <skos:prefLabel xml:lang="en">inventory managers</skos:prefLabel>
  <foaf:name>inventory managers</foaf:name>
  <rdfs:label xml:lang="en">inventory managers (JP 1-02)</rdfs:label>

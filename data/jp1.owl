  <foaf:name>riot control agent</foaf:name>
  <rdfs:label xml:lang="en">riot control agent (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Any chemical, not listed in a schedule of the Convention on the Prohibition of the Development, Production, Stockpiling and Use of Chemical Weapons and on their Destruction which can produce rapidly in humans sensory irritation or disabling physical effects which disappear within a short time following termination of exposure.</skos:definition>
  <skos:altLabel>RCA</skos:altLabel>
  <skos:related rdf:resource="#chemical_warfare"/>
  <skos:inScheme rdf:resource="#JP_3-11Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="rising_mine">
  <dc:source rdf:resource="#JP_1-02DocPage273"/>
  <skos:prefLabel xml:lang="en">rising mine</skos:prefLabel>

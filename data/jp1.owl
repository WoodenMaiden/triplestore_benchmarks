  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A sandbank or bar that makes water shoal; i.e., a sand-bank that is not rocky and on which there is a water depth of 6 fathoms or less.</skos:definition>
  <skos:inScheme rdf:resource="#JP_4-01.6Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>

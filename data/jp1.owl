  <dc:source rdf:resource="#JP_1-02DocPage185"/>
  <skos:prefLabel xml:lang="en">laser illuminator</skos:prefLabel>
  <foaf:name>laser illuminator</foaf:name>
  <rdfs:label xml:lang="en">laser illuminator (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>

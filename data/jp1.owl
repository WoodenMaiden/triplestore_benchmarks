<skos:Concept rdf:ID="amphibious_squadron">
  <dc:source rdf:resource="#JP_1-02DocPage18"/>
  <skos:prefLabel xml:lang="en">amphibious squadron</skos:prefLabel>
  <foaf:name>amphibious squadron</foaf:name>
  <rdfs:label xml:lang="en">amphibious squadron (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A tactical and administrative organization composed of amphibious assault shipping to transport troops and their equipment for an amphibious assault operation.</skos:definition>
  <skos:altLabel>PHIBRON</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_3-02Scheme"/>

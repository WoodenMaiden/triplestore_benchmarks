  <skos:prefLabel xml:lang="en">air delivery</skos:prefLabel>
  <foaf:name>air delivery</foaf:name>
  <rdfs:label xml:lang="en">air delivery (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">See airdrop; airland; air movement.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-17Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="delegation_of_authority">
  <dc:source rdf:resource="#JP_1-02DocPage87"/>
  <skos:prefLabel xml:lang="en">delegation of authority</skos:prefLabel>
  <foaf:name>delegation of authority</foaf:name>
  <rdfs:label xml:lang="en">delegation of authority (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The action by which a commander assigns part of his or her authority commensurate with the assigned task to a subordinate commander. While ultimate responsibility cannot be relinquished, delegation of authority carries with it the imposition of a measure of responsibility. The extent of the authority delegated must be clearly stated. 87</skos:definition>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>


  <skos:prefLabel xml:lang="en">landing sequence table</skos:prefLabel>
  <foaf:name>landing sequence table</foaf:name>
  <rdfs:label xml:lang="en">landing sequence table (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A document that incorporates the detailed plans for ship-toshore movement of nonscheduled units.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-02Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>

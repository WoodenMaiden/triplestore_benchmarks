  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">In intelligence use, the process used to determine the asset authenticity, reliability, utility, suitability, and degree of control the case officer or others have.</skos:definition>
  <skos:inScheme rdf:resource="#JP_2-01.2Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="returned_to_military_control">

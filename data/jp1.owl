  <foaf:name>range marker</foaf:name>
  <rdfs:label xml:lang="en">range marker (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">(*) A single calibration blip fed onto the time base of a radial display. The rotation of the time base shows the single blips as a circle on the plan position indicator scope. It may be used to measure range.</skos:definition>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="range_markers">
  <dc:source rdf:resource="#JP_1-02DocPage262"/>
  <skos:prefLabel xml:lang="en">range markers</skos:prefLabel>
  <foaf:name>range markers</foaf:name>
  <rdfs:label xml:lang="en">range markers (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Two upright markers that may be lighted at night and placed so that, when aligned, the direction indicated assists in piloting. They may be used in amphibious operations to aid in beaching landing ships or craft.</skos:definition>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="rangers">
  <dc:source rdf:resource="#JP_1-02DocPage262"/>
  <skos:prefLabel xml:lang="en">Rangers</skos:prefLabel>
  <foaf:name>Rangers</foaf:name>
  <rdfs:label xml:lang="en">Rangers (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Rapidly deployable airborne light infantry organized and trained to conduct highly complex joint direct action operations in coordination with or in support of other special operations units of all Services.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-05Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="rapid_and_precise_response">

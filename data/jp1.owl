  <skos:prefLabel xml:lang="en">special operations-peculiar</skos:prefLabel>
  <foaf:name>special operations-peculiar</foaf:name>
  <rdfs:label xml:lang="en">special operations-peculiar (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">Equipment, material, supplies, and services required for special operations missions for which there is no Service-common requirement.</skos:definition>

    <rdf:first rdf:resource="#section_1"/>
    <rdf:rest>
     <rdf:List rdf:ID="section_list_1">
      <rdf:first rdf:resource="#section_2"/>
      <rdf:rest>
       <rdf:List rdf:ID="section_list_2">
        <rdf:first rdf:resource="#section_3"/>

  <skos:prefLabel xml:lang="en">Aegis</skos:prefLabel>
  <foaf:name>Aegis</foaf:name>
  <rdfs:label xml:lang="en">Aegis (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">A ship-based combat system that can detect, track, target, and engage air, surface, and subsurface threats, including ballistic missiles on some modified ships.</skos:definition>
  <skos:inScheme rdf:resource="#JP_3-01Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>


<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="amphibious_demonstration">
  <dc:source rdf:resource="#JP_1-02DocPage17"/>
  <skos:prefLabel xml:lang="en">amphibious demonstration</skos:prefLabel>
  <foaf:name>amphibious demonstration</foaf:name>

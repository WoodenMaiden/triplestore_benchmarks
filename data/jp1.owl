  <rdfs:label xml:lang="en">risk assessment (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The identification and assessment of hazards (first two steps of risk management process).</skos:definition>
  <skos:altLabel>RA</skos:altLabel>
  <skos:inScheme rdf:resource="#JP_3-07.2Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>
<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
<vs:term_status>Stable</vs:term_status>
</skos:Concept>

<skos:Concept rdf:ID="risk_management">
  <dc:source rdf:resource="#JP_1-02DocPage273"/>
  <skos:prefLabel xml:lang="en">risk management</skos:prefLabel>
  <foaf:name>risk management</foaf:name>
  <rdfs:label xml:lang="en">risk management (JP 1-02)</rdfs:label>
  <skos:inScheme rdf:resource="#JP_1-02Scheme"/>
  <skos:definition xml:lang="en">The process of identifying, assessing, and controlling risks arising from operational factors and making decisions that balance risk cost with mission benefits.</skos:definition>
  <skos:altLabel>RM</skos:altLabel>
  <skos:related rdf:resource="#risk"/>
  <skos:inScheme rdf:resource="#JP_3-0Scheme"/>
<rdfs:isDefinedBy rdf:resource="http://rdf.muninn-project.org/ontologies/jp1"/>

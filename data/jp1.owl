  <skos:related rdf:resource="#fire_support_area"/>
  <skos:related rdf:resource="#inner_transport_area"/>
  <skos:related rdf:resource="#sea_echelon_area"/>
  <skos:inScheme rdf:resource="#JP_3-02Scheme"/>
